def main():
    n = list(input())
    num_digitos = len(n)
    reparticao = 10
    reverso = []
    myString = "".join(n)
    n = int(myString)
    for i in range(num_digitos):
        inverso = n % reparticao
        reverso.append(inverso)
        n = n // reparticao
    myString2 = "".join(str(reverso))
    revertido = int(myString2)
    if n == revertido:
        print("oi")
    else:
        print("io")
main()
